{
  "name": "qwind",
  "description": "A template to make your website using Qwik + Tailwind CSS.",
  "version": "0.108.0",
  "private": true,
  "scripts": {
    "build": "qwik build && jampack ./dist",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adaptor'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "postbuild": "npm run subfont",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "subfont": "subfont -i --inline-css --no-fallbacks --silent --root dist",
    "qwik": "qwik"
  },
  "dependencies": {
    "@builder.io/qwik": "1.2.13",
    "@builder.io/qwik-city": "1.2.13",
    "@fontsource/inter": "5.0.12",
    "@unpic/qwik": "0.0.27",
    "gray-matter": "4.0.3",
    "markdown-it": "13.0.2",
    "node-fetch": "3.3.2"
  },
  "devDependencies": {
    "@divriots/jampack": "0.21.1",
    "@tailwindcss/typography": "0.5.10",
    "@types/eslint": "8.44.3",
    "@types/markdown-it": "13.0.2",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "autoprefixer": "10.4.16",
    "eslint": "8.51.0",
    "eslint-plugin-qwik": "1.2.13",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "subfont": "7.1.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "vite": "4.4.11",
    "vite-tsconfig-paths": "4.2.1"
  },
  "engines": {
    "node": ">=15.0.0"
  }
}
